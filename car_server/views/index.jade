extends layout

block content
	.container
		h1 Levels
		h3 Carburant level
		.progress
			.progress-bar.progress-bar-danger.progress-bar-striped.active(role='progressbar', aria-valuenow='45', aria-valuemin='0', aria-valuemax='100', style='width: 10%')
		h3 Oil level
		.progress
			.progress-bar.progress-bar-success.progress-bar-striped.active(role='progressbar', aria-valuenow='45', aria-valuemin='0', aria-valuemax='100', style='width: 80%')
		h3 Windshield washer fluid
		.progress
			.progress-bar.progress-bar-warning.progress-bar-striped.active(role='progressbar', aria-valuenow='45', aria-valuemin='0', aria-valuemax='100', style='width: 40%')
